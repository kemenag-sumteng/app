<%= render "partial/navigation" %>
<p id="notice"><%= notice %></p>

<h1>E Data Keagamaan Katolik</h1>

<table>
  <thead>
    <tr>
      <th>Judul</th>
      <th>Keterangan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @e_data_keagamaan_katolik.each do |e_data_keagamaan_katolik| %>
      <tr>
        <td><%= e_data_keagamaan_katolik.judul %></td>
        <td><%= e_data_keagamaan_katolik.keterangan %></td>
        <td><%= link_to 'Tampilkan', e_data_keagamaan_katolik %></td>
        <td><%= link_to 'Edit', edit_e_data_keagamaan_katolik_path(e_data_keagamaan_katolik) %></td>
        <td><%= link_to 'Hapus', e_data_keagamaan_katolik, method: :delete, data: { confirm: 'Anda yakin?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Membuat E Data Keagamaan Katolik', new_e_data_keagamaan_katolik_path %>
